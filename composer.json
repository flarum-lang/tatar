{
	"name": "flarum-lang/tatar",
	"type": "flarum-extension",
	"description": "Tatar language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/tatar",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"tt-locale",
		"tatar-locale",
		"tatar"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/tatar/issues",
		"source": "https://github.com/flarum-lang/tatar"
	},
	"require": {
		"flarum/core": "^1.7"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Tatar",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "tt",
			"title": "Tatar"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
